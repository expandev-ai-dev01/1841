{
  "name": "foodtrack-backend",
  "version": "1.0.0",
  "description": "FoodTrack Backend API - Sistema de controle de compras de alimentos",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/server.js",
    "migrate": "ts-node src/migrations/run-migrations.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix"
  },
  "keywords": [
    "foodtrack",
    "food",
    "purchases",
    "tracking",
    "api",
    "rest"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "dotenv": "^16.3.1",
    "mssql": "^10.0.1",
    "zod": "^3.22.4",
    "morgan": "^1.10.0",
    "patch-package": "^8.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/morgan": "^1.9.9",
    "@types/mssql": "^9.1.5",
    "typescript": "^5.3.3",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.8",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11",
    "ts-jest": "^29.1.1"
  }
}